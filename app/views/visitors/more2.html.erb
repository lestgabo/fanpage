<% content_for :title do %>Emma Watson<% end %>
<% content_for :description do %>Fan page for Emma Watson<% end %>

<div class="container text-center">
  <div class="row">
    <!--<h3 class="text-center">Scraped from Reddit's /r/EmmaWatson top 25 all time.</h3>-->
    <!-- specifically from url => 'https://www.reddit.com/r/EmmaWatson/top/?sort=top&t=all' -->
    <h2 class="text-center">The Emma Watsons below are NOT SAFE FOR WORK! <strong style="color:red;">(NSFW)</strong>
    </h2>
    <br>
    <br>
    <strong>
      <%= link_to root_path, class:"btn-back-to-home btn-danger" do %>
        Back to home page.
      <% end %>
    </strong>
    <br>
    <br>
    <br>
 
    <% @test5.each do |title, link| %>
    <h2>
      <%= title %> 
      <br/>
      <% if link.include?("a/") %>
        <blockquote class="imgur-embed-pub" lang="en" data-id=<%=link%>></blockquote><script async src="//s.imgur.com/min/embed.js" charset="utf-8"></script>
      <% elsif link[0,4] != "http" && link[0,5] != "https" %>
        <div style='position:relative;padding-bottom:57%'><iframe src='https://gfycat.com/ifr/<%=link%>' frameborder='0' scrolling='no' width='100%' height='100%' style='position:absolute;top:0;left:0;' allowfullscreen></iframe></div>
      <% else %>
        <%= image_tag "#{link}" %> 
      <% end %>
    <br>
     
    <% end %>
    
    <br>
    Use the contact page to request for more celebrities.
    <br>
    <br>
    <%= link_to root_path, class:"btn-back-to-home btn-primary" do %>
      Back to home page.
    <% end %>
    </h2>
  </div>
</div>
